#include<stdio.h>
 
int arr[9];
int used[9];
int N, M;
 
void backtracking(int idx) {
    if (idx == M) {
        for (int i = 0; i < M; i++)
            printf("%d ", arr[i]);
        printf("\n");
        return;
    }
 
    for (int i = 1; i <= N; i++) {
        if (used[i] == 0) {
            arr[idx] = i;
            used[i] = 1;
            backtracking(idx + 1);
            used[i] = 0;
        }
    }
}

int main(void) {
    scanf("%d %d", &N, &M);
    backtracking(0);
    return 0;
}
 

